name "lyaml"
description "Lightweight YAML parser"
authors "0-v-0"
copyright "Copyright Â© 2022 0-v-0"
license "MIT"
dependency "tame" version="~>0.0.1"
sourcePaths "lyaml"
importPaths "."
configuration "library" {
	targetType "library"
	dflags "-preview=shortenedMethods" platform="ldc"
}
configuration "lshared" {
	targetType "library"
	dflags "-preview=shortenedMethods" "-link-defaultlib-shared" "-L/force:unresolved" platform="ldc"
}
